{
  "compilerOptions": {
    "target": "es6",
    "moduleResolution": "node",
    "module": "esnext",
    "jsx": "react",
    "lib": [
      "dom",
      "es2015",
      "es2016",
      "es2017",
      "dom.iterable",
      "scripthost"
    ],
    "types": [],
    "allowSyntheticDefaultImports": true,
    "alwaysStrict": false,
    "strictNullChecks": false,
    "noImplicitAny": false,
    "noImplicitReturns": false,
    "noImplicitThis": false,
    "noUnusedLocals": false,
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "inlineSources": true,
    "sourceMap": true,
    "outDir": "dist",
    "baseUrl": ".",
    "paths": {
      "helpers/*": [
        "app/src/helpers/*"
      ],
      "filters/*": [
        "app/src/filters/*"
      ],
      "components/*": [
        "app/src/components/*"
      ],
      "modules/*": [
        "app/src/modules/*"
      ],
      "configs/*": [
        "app/src/configs/*"
      ],
      "containers/*": [
        "app/src/containers/*"
      ],
      "services/*": [
        "app/src/services/*"
      ],
      "xmpp/*": [
        "app/src/xmpp/*"
      ],
      "ajaxRequests/*": [
        "app/src/ajax-requests/*"
      ],
      "scss/*": [
        "app/assets/styles/*"
      ],
      "requests/*": [
        "app/src/requests/*"
      ]
    }
  },
  "files": [
    "app/index.tsx"
  ],
  "include": [
    "app/**/*.ts",
    "app/**/*.tsx",
    "test/**/*.ts",
    "test/**/*.tsx",
    "node_modules/@types/**/*.d.ts"
  ],
  "exclude": [
    "dist"
  ]
}
